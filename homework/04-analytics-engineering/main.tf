terraform {
  required_providers {
    google = {
      source  = "hashicorp/google"
      version = "5.6.0"
    }
  }
}

provider "google" {
  credentials = file(var.credentials)
  project     = var.project
  region      = var.region
}


resource "google_bigquery_dataset" "dataset" {
  dataset_id = var.gcp_bigquery_dataset
  location   = var.location
}
